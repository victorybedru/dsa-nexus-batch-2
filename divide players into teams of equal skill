class Solution(object):
    def dividePlayers(self, skill):
        skill.sort()
        left=0
        right= len(skill)-1
        target=skill[left]+ skill[right]
        res=0
        while left<right:
            sum1=0
            if skill[left]+skill[right]!=target:
                return -1
            res+= skill[left]*skill[right]
            left+=1
            right-=1
        return res
