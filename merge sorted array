class Solution(object):
    def merge(self, nums1, m, nums2, n):
        count=0
        while 0 in nums1:
            nums1.remove(0)
            count+=1
        remain=count-n
        for i in range(remain):
            nums1.append(0)
        for i in nums2:
            nums1.append(i)
        nums1.sort()
        print(nums1)
