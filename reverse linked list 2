class Solution:
    def reverseBetween(self, head, left, right):
        dummy = ListNode(0)
        dummy.next = head
        prev = dummy
        for i in range(left - 1):
            prev = prev.next
        curr = prev.next
        for i in range(right - left):
            tmp = curr.next
            curr.next = tmp.next
            tmp.next = prev.next
            prev.next = tmp
        return dummy.next
